{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "apiVersion": "v1",
            "kind": "BuildConfig",
            "metadata": {
                "name": "rh-massage-build"
            },
            "spec": {
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "rh-massages:latest"
                    }
                },
                "source": {
                    "git": {
                        "uri": "https://github.com/TheSprtCz/rh-massages.git"
                    },
                    "type": "Git"
                },
                "strategy": {
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStream",
                            "name": "s2i-java"
                        },
                        "runtimeArtifacts": [
                            {
                                "destinationDir": ".",
                                "sourcePath": "/tmp/src/dropwizard-api/target/Massages-1.3.0.jar"
                            }
                        ],
                        "runtimeImage": {
                            "kind": "DockerImage",
                            "name": "openjdk:8-jre"
                        }
                    },
                    "type": "Source"
                },
                "triggers": [
                    {
                        "github": {
                            "secret": "openshift"
                        },
                        "type": "GitHub"
                    }
                ]
            }
        }
    ]
}
